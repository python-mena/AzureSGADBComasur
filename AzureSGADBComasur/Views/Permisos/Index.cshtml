@model IEnumerable<AzureSGADBComasur.Models.PermisosTable>
@{
    ViewBag.Title = "Index";
}
<h2>Index</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdPermiso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NombrePermiso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumerodePermiso)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdPermiso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NombrePermiso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumerodePermiso)
            </td>
        </tr>
    }
</table>
